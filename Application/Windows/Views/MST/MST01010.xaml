<fw:WindowMasterMainteBase
    xmlns:fw="clr-namespace:KyoeiSystem.Framework.Windows.ViewBase;assembly=KyoeiSystem.Framework.Windows"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:vm="clr-namespace:KyoeiSystem.Framework.Windows.ViewBase;assembly=KyoeiSystem.Framework.Windows"
    xmlns:ctls="clr-namespace:KyoeiSystem.Framework.Windows.Controls;assembly=KyoeiSystem.Framework.Windows.Controls"
    xmlns:uc="clr-namespace:KyoeiSystem.Application.Windows.Views"
    xmlns:Data="clr-namespace:System.Data;assembly=System.Data"
    x:Class="KyoeiSystem.Application.Windows.Views.MST01010"
    mc:Ignorable="d" AllowsTransparency="False" Language="ja-JP"
    Height="980" Width="970"
    MinHeight="980" MinWidth="1024"
    WindowStartupLocation="CenterOwner"
    Title="取引先マスタ保守(MST01010)"
    Loaded="Window_Loaded" Closed="Window_Closed">

  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="auto" />
      <RowDefinition Height="*" />
    </Grid.RowDefinitions>

    <!-- リボン -->
    <Ribbon HorizontalAlignment="Stretch" Grid.ColumnSpan="2" VerticalAlignment="Stretch">
      <!-- タブ -->
      <RibbonTab Header="ホーム" KeyTip="h" Height="75">
        <RibbonGroup Width="90" >
          <!-- 処理モード表示用に空けておく -->
        </RibbonGroup>

        <RibbonGroup Header="F1" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton LargeImageSource="../Images/15マスタ照会.png" Margin="0,-1,0,0"
                        ToolTipTitle="マスタ照会(F1)" ToolTipDescription="入力済みのデータを検索します。"
                        ToolTipImageSource="../Images/15マスタ照会.png"
                        Label="ﾏｽﾀ照会" Click="RibbonButton_Click" KeyTip="F1" Height="50"
          />
        </RibbonGroup>
        <RibbonGroup Header="F2" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F3" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F4" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F5" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F6" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F7" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F8" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>
        <RibbonGroup Header="F9" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton LargeImageSource="../Images/24登録.png" Click="RibbonButton_Click" KeyTip="F9" Height="50"
                                  Visibility="{Binding Path=DataUpdateVisible, Mode=TwoWay, NotifyOnSourceUpdated=True, NotifyOnTargetUpdated=True}"
                                  Label="登録" ToolTipTitle="データ登録(F9)" ToolTipDescription="入力データの登録を行います" ToolTipImageSource="../Images/24登録.png" />
        </RibbonGroup>
        <RibbonGroup Header="F10" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton LargeImageSource="../Images/26クリア.png" Click="RibbonButton_Click" KeyTip="F10" Height="50"
                                  Label="入力取消" ToolTipTitle="入力取消(F10)" ToolTipDescription="入力の取消しを行います" ToolTipImageSource="../Images/26クリア.png" />
        </RibbonGroup>
        <RibbonGroup Header="F11" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton LargeImageSource="../Images/27終了.png" Click="RibbonButton_Click" KeyTip="F11" Height="50"
                                  Label="終了" ToolTipTitle="終了(F11)" ToolTipDescription="終了します" ToolTipImageSource="../Images/27終了.png" />
        </RibbonGroup>
        <RibbonGroup Header="F12" KeyTip="MASTER" Background="{StaticResource ResourceKey=RibbonFunctionBackground}">
          <RibbonButton Visibility="Hidden" Label="" ToolTipTitle="" ToolTipDescription="" />
        </RibbonGroup>

      </RibbonTab>

      <RibbonTab Header="ヘルプ" Background="Gray">
        <RibbonGroup Header="サポート">
          <RibbonButton LargeImageSource="../Images/30マニュアル.png" KeyTip="MANUAL" Click="RibbonButton_Click"
                                  Label="ﾏﾆｭｱﾙ" ToolTipTitle="取扱説明書" ToolTipDescription="取り扱い説明書を開きます" />
          <RibbonButton LargeImageSource="../Images/29遠隔サポート.png" KeyTip="SUPPORT" Click="RibbonButton_Click"
                                  Label="お問合せ" ToolTipTitle="お問合せ" ToolTipDescription="Web上の｢お問合せ｣ページに遷移します" />
          <RibbonButton LargeImageSource="../Images/QandA.png" KeyTip="FAQ" Click="RibbonButton_Click"
                                  Label="QandA" ToolTipTitle="QandA" ToolTipDescription="Web上の｢よくあるご質問｣ページに遷移します"/>
          <RibbonButton LargeImageSource="../Images/13共栄Webリンク.png"  KeyTip="KYOEI" Click="RibbonButton_Click"
                                  Label="共栄ｼｽﾃﾑ" ToolTipTitle="共栄システムHP" ToolTipDescription="共栄システムのホームページを開きます"/>
        </RibbonGroup>
      </RibbonTab>

      <RibbonTab Header="実施状況入力" Background="Gray" ContextualTabGroupHeader="入力補助" >
        <RibbonGroup Header="従来ファンクションキー">
          <RibbonButton LargeImageSource="../Images/16マスタ入力.png" KeyTip="d"
                                  Label="行追加" ToolTipTitle="行追加(F5)" ToolTipDescription="売上の履歴を表示します"  ToolTipImageSource="../Images/16マスタ入力.png"/>
          <RibbonButton LargeImageSource="../Images/24登録.png" Click="RibbonButton_Click" KeyTip="e"
                                  Label="マスタ登録" ToolTipTitle="伝票登録(F9)" ToolTipDescription="伝票の登録を行います" ToolTipImageSource="../Images/24登録.png"/>
          <RibbonButton LargeImageSource="../Images/26クリア.png" Click="RibbonButton_Click" KeyTip="f"
                                  Label="入力取消し" ToolTipTitle="入力取消(F12)" ToolTipDescription="入力の取消しを行います" ToolTipImageSource="../Images/26クリア.png"/>
          <RibbonButton LargeImageSource="../Images/27終了.png" Click="RibbonButton_Click" KeyTip="g"
                                  Label="終了" ToolTipTitle="入力取消(F11)" ToolTipDescription="終了します" ToolTipImageSource="../Images/27終了.png"/>
        </RibbonGroup>
      </RibbonTab>

      <RibbonTab Header="一括入力" Background="Gray" ContextualTabGroupHeader="入力補助">
        <RibbonGroup Header="従来ファンクションキー">
          <RibbonButton LargeImageSource="../Images/22リスト.png" KeyTip="d"
                                  Label="プレビュー" ToolTipTitle="プレビュー(F5)" ToolTipDescription="売上の履歴を表示します"  ToolTipImageSource="../Images/22リスト.png"/>
          <RibbonButton LargeImageSource="../Images/24登録.png" Click="RibbonButton_Click" KeyTip="e"
                                  Label="印刷" ToolTipTitle="印刷(F9)" ToolTipDescription="伝票の登録を行います" ToolTipImageSource="../Images/24登録.png"/>
          <RibbonButton LargeImageSource="../Images/24登録.png" Click="RibbonButton_Click" KeyTip="e"
                                  Label="マスタ登録" ToolTipTitle="マスタ登録(F9)" ToolTipDescription="伝票の登録を行います" ToolTipImageSource="../Images/24登録.png"/>
          <RibbonButton LargeImageSource="../Images/26クリア.png" Click="RibbonButton_Click" KeyTip="f"
                                  Label="入力取消し" ToolTipTitle="入力取消(F12)" ToolTipDescription="入力の取消しを行います" ToolTipImageSource="../Images/26クリア.png"/>
          <RibbonButton LargeImageSource="../Images/27終了.png" Click="RibbonButton_Click" KeyTip="g"
                                  Label="終了" ToolTipTitle="入力取消(F11)" ToolTipDescription="終了します" ToolTipImageSource="../Images/27終了.png"/>
        </RibbonGroup>
      </RibbonTab>

    </Ribbon>

    <Grid Grid.Row="0" VerticalAlignment="Bottom">
      <Grid.RowDefinitions>
        <RowDefinition Height="74" />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="90" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>

      <!-- 編集モード領域 -->
      <DockPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" Margin="2"
                 Background="{Binding MaintenanceModeBackground}">
        <Label Content="{Binding MaintenanceMode}" Foreground="{Binding MaintenanceModeForeground}" 
               FontFamily="メイリオ" FontSize="24" FontWeight="Medium"
               HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalAlignment="Center" VerticalContentAlignment="Center"
        />
      </DockPanel>

      <!-- エラーメッセージ領域 -->
      <!-- プロパティErrorMessageが空白以外である間だけ表示されます。 -->
      <Label Grid.Row="0" Grid.Column="1" VerticalAlignment="Bottom" Background="Yellow" Foreground="Red" Margin="2,0,2,0"
             Content="{Binding ErrorMessage}" Visibility="{Binding ErrorMessageVisibility}"
      />

    </Grid>

    <!--ヘッダー-->
    <Grid Grid.Row="1" Background="AliceBlue" Margin="5">

      <Grid Margin="15">
        <Grid.RowDefinitions>
          <RowDefinition Height="30" />
          <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="55*" />
          <ColumnDefinition Width="67*" />
        </Grid.ColumnDefinitions>

        <!-- 取引先コード -->
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
          </Grid.ColumnDefinitions>

          <ctls:UcLabelTextBox x:Name="TORI_CODE" Label_Context="取引先コード" cWidth="60" Label_Width="100" IsKeyItem="True"
                               ImeType="Off" cMaxLength="4" HorizontalAlignment="Left" ValidationType="Integer" cVerticalContentAlignment="Center"
                               Text="{Binding 取引先コード, Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
                               GotFocus="supCode_GotFocus" LostFocus="supCode_LostFocus" PreviewMouseDown="supCode_PreviewMouseDown"
          />

          <Label Content="―" Width="20" HorizontalAlignment="Left" Grid.Column="1"/>

          <ctls:UcTextBox x:Name="TORI_EDA" IsKeyItem="True" Grid.Column="2" cWidth="30"
                          ImeType="Off" cMaxLength="2" HorizontalAlignment="Left" ValidationType="Integer" cVerticalContentAlignment="Center"
                          Text="{Binding 枝番, Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
                          PreviewKeyDown="TORI_EDA_PreviewKeyDown"
          />
        </Grid>

        <!-- 取引先基本情報 -->
        <Grid x:Name="grdLeft" VerticalAlignment="Top" Grid.Row="1" Height="760" >
          <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="40" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
          </Grid.RowDefinitions>

          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
          </Grid.ColumnDefinitions>

          <ctls:UcLabelTextBox x:Name="TORI_NM_1" Label_Context="正式名称" Grid.Row="0" Label_Width="100"
                               ImeType="Native" cMaxLength="50" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[得意先名１], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox x:Name="TORI_NM_2" Label_Context="正式名称２" Grid.Row="1" Label_Width="100"
                               ImeType="Native" cMaxLength="50" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[得意先名２], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox x:Name="TORI_DEPT_NM" Label_Context="部課名称" Grid.Row="2" Label_Width="100"
                               ImeType="Native" cMaxLength="50" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[部課名称], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox x:Name="TORI_RNM" Label_Context="略　称" Grid.Row="3" Label_Width="100"
                               ImeType="Native" cMaxLength="30" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[略称名], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox x:Name="YUBIN_NO" Label_Context="郵便番号" Grid.Row="4" Label_Width="100"
                               ImeType="Off" cMaxLength="8" HorizontalAlignment="Left" cWidth="100" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[郵便番号], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
                               SourceUpdated="YUBIN_NO_SourceUpdated"
          />
          <ctls:UcLabelTextBox x:Name="ADDRESS_1" Label_Context="住所１" Grid.Row="5" Label_Width="100"
                               ImeType="Native" cMaxLength="50" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[住所１], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox x:Name="ADDRESS_2" Label_Context="住所２" Grid.Row="6" Label_Width="100"
                               ImeType="Native" cMaxLength="50" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[住所２], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <ctls:UcLabelTextBox x:Name="TEL" Label_Context="ＴＥＬ" Grid.Row="7" Label_Width="100"
                               ImeType="Off" cMaxLength="15" HorizontalAlignment="Left" cWidth="100" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[電話番号], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <ctls:UcLabelTextBox x:Name="FAX" Label_Context="ＦＡＸ" Grid.Row="8" Label_Width="100"
                               ImeType="Off" cMaxLength="15" HorizontalAlignment="Left" cWidth="100" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[ＦＡＸ], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <ctls:UcLabelTextBox x:Name="TORI_KNM" Label_Context="カナ名称" Grid.Row="9" Label_Width="100"
                               ImeType="HankakuKatakana" cMaxLength="20" HorizontalAlignment="Left" Width="430" cVerticalContentAlignment="Center"
                               Text="{Binding RowM01TOK[かな読み], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <ctls:UcLabelTwinTextBox
              x:Name="JIS_COMPANY" Label_Context="担当会社コード" Grid.Row="10" Label_Width="100" DataAccessName="M70_JIS" DataAccessMode="On" IsKeyItem="False"
              VerticalAlignment="Top" HorizontalAlignment="Left" Height="30"
              Label1Visibility="Collapsed" Label2Visibility="Collapsed"
              Text1Width="40" Text1MaxLength="3" Text1IMEType="Off" Text1ValidationType="Integer" Text1VerticalContentAlignment="Center"
              Text2Width="286" Text2IsReadOnly="True" Text2Background="Transparent" Text2BorderThickness="0" Text2VerticalContentAlignment="Center"
              Text1="{Binding RowM01TOK[担当会社コード], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <uc:M01_TOK_TextBox
            x:Name="KAISYA_CD" Grid.Row="11" HorizontalAlignment="Left" Margin="0,0,240,0"
            DataAccessName="M01_TOK" DataAccessMode="On"
            Label_Context="集約取引先コード" Label_Width="100"
            Label1Text="－" Label1Visibility="Visible"
            Text1Width="60" Text1MaxLength="4" Text1IMEType="Off" Text1ValidationType="Integer" Text1VerticalContentAlignment="Center"
            Text2Width="30" Text2MaxLength="2" Text2IMEType="Off" Text2ValidationType="Integer" Text2VerticalContentAlignment="Center"
            Text1="{Binding RowM01TOK[集約取引先コード], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
            Text2="{Binding RowM01TOK[集約取引先枝番], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

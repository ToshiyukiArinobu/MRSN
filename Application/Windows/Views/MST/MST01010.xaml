              Label1Text="－" Label1Visibility="Visible"
              Text1Width="60" Text1MaxLength="4" Text1IMEType="Off" Text1ValidationType="Integer" Text1VerticalContentAlignment="Center"
              Text2Width="30" Text2MaxLength="2" Text2IMEType="Off" Text2ValidationType="Integer" Text2VerticalContentAlignment="Center"
              Text1="{Binding RowM01TOK[集約取引先コード], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
              Text2="{Binding RowM01TOK[集約取引先枝番], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
-->
          <ctls:UcLabelTextBox
              x:Name="SHUKEI_CD_1" Label_Context="集計ｺｰﾄﾞ①" Grid.Row="12" Label_Width="100" cWidth="60"
              ImeType="Off" cMaxLength="5" HorizontalAlignment="Left" ValidationType="Integer" cVerticalContentAlignment="Center"
              Text="{Binding RowM01TOK[集計コード１], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox
              x:Name="SHUKEI_CD_2" Label_Context="集計ｺｰﾄﾞ②" Grid.Row="13" Label_Width="100" cWidth="60"
              ImeType="Off" cMaxLength="5" HorizontalAlignment="Left" ValidationType="Integer" cVerticalContentAlignment="Center"
              Text="{Binding RowM01TOK[集計コード２], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />
          <ctls:UcLabelTextBox
              x:Name="SHUKEI_CD_3" Label_Context="集計ｺｰﾄﾞ③" Grid.Row="14" Label_Width="100" cWidth="60"
              ImeType="Off" cMaxLength="5" HorizontalAlignment="Left" ValidationType="Integer" cVerticalContentAlignment="Center"
              Text="{Binding RowM01TOK[集計コード３], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <Grid Grid.Row="15" Grid.RowSpan="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="100" />
              <ColumnDefinition Width="auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
              <RowDefinition Height="*" />
              <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <ctls:UcLabel LabelText="自社備考" Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="58" Width="100" />
            <ctls:UcLabelTwinTextBox
                x:Name="BIKO_1" Grid.Row="0" Grid.Column="1" DataAccessName="M11_TEK" DataAccessMode="On" IsKeyItem="False" HorizontalAlignment="Left"
                Label_Visibility="Collapsed" Label1Visibility="Collapsed" Label2Visibility="Collapsed"
                Text1Width="40" Text1MaxLength="5" Text1IMEType="Off" Text1ValidationType="Integer" Text1VerticalContentAlignment="Center"
                Text2Width="285" Text2MaxLength="50" Text2IMEType="Native" Text2VerticalContentAlignment="Center"
                Text2="{Binding RowM01TOK[自社備考１], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
            />
            <ctls:UcLabelTwinTextBox
                x:Name="BIKO_2" Grid.Column="1" Grid.Row="1" DataAccessName="M11_TEK" DataAccessMode="On" IsKeyItem="False" HorizontalAlignment="Left"
                Label_Visibility="Collapsed" Label1Visibility="Collapsed" Label2Visibility="Collapsed"
                Text1Width="40" Text1MaxLength="5" Text1IMEType="Off" Text1ValidationType="Integer" Text1VerticalContentAlignment="Center"
                Text2Width="285" Text2MaxLength="50" Text2IMEType="Native" Text2VerticalContentAlignment="Center"
                Text2="{Binding RowM01TOK[自社備考２], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
            />
          </Grid>
          
          <Grid Grid.Row="17">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="0.5*" />
              <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <ctls:UcLabelTextBox
                x:Name="DEL_FLG" Grid.Row="17" HorizontalAlignment="Left"
                Label_Context="論理削除" Label_Width="100"
                ValidationType="Integer" ImeType="Off"
                cWidth="30" cMaxLength="1" cVerticalContentAlignment="Center"
                Text="{Binding RowM01TOK[論理削除], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
            />
            <Label Content="&quot;0&quot;は使用中、&quot;9&quot;は削除済みになります。" HorizontalAlignment="Left" Grid.Row="17" Grid.Column="2" Width="267" />
          </Grid>
          
          <ctls:UcLabelTextBox
              x:Name="DEL_DT" Grid.Row="18" Focusable="False"
              Label_Context="削除日" Label_Width="100"
              cWidth="305" cIsReadOnly="True" cBorderThickness="0" cBackground="Transparent"
              cHorizontalContentAlignment="Right" cVerticalContentAlignment="Center"
              Text="{Binding RowM01TOK[削除日時], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, StringFormat='yyyy/MM/dd HH:mm:ss'}"
          />
          <ctls:UcLabelTwinTextBox
              x:Name="DEL_STAFF" Grid.Row="19" HorizontalAlignment="Left"
              DataAccessName="M72_TNT" DataAccessMode="On"
              Label_Context="削除者" Label_Width="100"
              Label1Visibility="Collapsed" Label2Visibility="Collapsed"
              Text1Visibility="Collapsed" Text1IsReadOnly="True" Text1AcceptsTab="False"
              Text2IsReadOnly="True" Text2Width="286" Text2VerticalContentAlignment="Center" Text2AcceptsTab="False"
              Text2BorderThickness="0" Text2Background="Transparent"
              Text1="{Binding RowM01TOK[削除者], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}"
          />

          <ctls:UcLabelTextBox
              x:Name="ADD_DT" Grid.Row="20" Focusable="False"
              Label_Context="登録日" Label_Width="100"
              cWidth="305" cIsReadOnly="True" cBorderThickness="0" cBackground="Transparent"
              cHorizontalContentAlignment="Right" cVerticalContentAlignment="Center"
              Text="{Binding RowM01TOK[登録日時], Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, StringFormat='yyyy/MM/dd HH:mm:ss'}"
          />
          <ctls:UcLabelTwinTextBox
              x:Name="ADD_STAFF" Grid.Row="21" HorizontalAlignment="Left"

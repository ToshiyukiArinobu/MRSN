                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"/>
                    </Grid.RowDefinitions>

          <WrapPanel Grid.Column="1">
                        <ctls:UcLabelTwinTextBox Label_Context="車輌ID" Label_Width="98" Text1MaxLength="5" Text1TextAlignment="Right"

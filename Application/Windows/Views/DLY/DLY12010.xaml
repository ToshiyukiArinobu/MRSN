          		Text1Width="80" DataAccessName="M05_CAR" OutputColumnName="車輌番号" Text1MaxLength="5" Text1ValidationType="Integer" Text1TextAlignment="Right" Text2IsReadOnly="True"
          		Text1="{Binding 車輌ID, Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, UpdateSourceTrigger=LostFocus}"

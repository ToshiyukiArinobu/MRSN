            		SelectedValue="{Binding 検索日付選択, Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"/>
              <ctls:UcTextBox Grid.Column="2" Name="textbox検索日付From"

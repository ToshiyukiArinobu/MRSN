                                       Text2="{Binding Path=デジタコ名, Mode=TwoWay, NotifyOnSourceUpdated=True, NotifyOnTargetUpdated=True}"
                                       />

              <ctls:UcLabelTextRadioButton cGroupName="SyokusyuBunrui" Grid.Row="3" Grid.ColumnSpan="2" Label_Content="職業分類" Label_Width="108" Margin="1" RadioViewCount="Six" RadioOne_Content="未" RadioSecond_Content="常時選任運転者" RadioThird_Content="運転者" RadioFourth_Content="職員" RadioFifth_Content="整備士" RadioSixth_Content="その他" RadioSelectButton="One"
                                   Text="{Binding Path=RowM04[職種分類区分], Mode=TwoWay, NotifyOnSourceUpdated=True, NotifyOnTargetUpdated=True}"
